@model Recipes.ViewModels.BlogEntryViewModel

@{ Layout = null; }

<script type="text/javascript">
    jQuery(function ($) {
        $("#treeview2").jstree({ "plugins": ["themes", "html_data"] });
    });
</script>

<div class="blogheader">
<h2>Digital Biology Blog</h2>
</div>
<div id="treeview2" class="treeview">
    @MvcHtmlString.Create(Html.RenderTree(Model.BlogEntries, x => x.Name))
</div>